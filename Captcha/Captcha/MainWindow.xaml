<Window x:Class="Captcha.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Captcha"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="qt" HorizontalAlignment="Left" Height="44" Margin="73,87,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="143" FontSize="16"/>
        <Label Content="Quantity of symbols:" HorizontalAlignment="Left" Height="32" Margin="64,55,0,0" VerticalAlignment="Top" Width="174" FontSize="16"/>
        <Button x:Name="gen" Content="Generate" HorizontalAlignment="Left" Height="88" Margin="460,55,0,0" VerticalAlignment="Top" Width="226" FontSize="16" Click="gen_Click"/>
        <Label x:Name="res" Content="" HorizontalAlignment="Left" Height="68" Margin="167,177,0,0" VerticalAlignment="Top" Width="390" FontSize="24" />
        <TextBox x:Name="check1" HorizontalAlignment="Left" Height="51" Margin="207,250,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="306" FontSize="24"/>
        <Button x:Name="check" Content="Check" HorizontalAlignment="Left" Height="51" Margin="557,250,0,0" VerticalAlignment="Top" Width="117" FontSize="20" Click="check_Click"/>

    </Grid>
</Window>
